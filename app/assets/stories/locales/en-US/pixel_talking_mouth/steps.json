{
    "steps": [
        {
            "banner": {
                "text": "Let's use the microphone to control an Animation to make a face talk.",
                "next_button": true
            },
            "beacon": {
                "target": "banner-button",
                "offset": 0
            }
        },
        {
            "banner": {
                "text": "We're animating something so we need a loop to run every frame, open the Control tray"
            },
            "beacon": {
                "target": {
                    "category": "control"
                }
            },
            "validation": {
                "blockly": {
                    "open-flyout": "control"
                }
            }
        },
        {
            "banner": {
                "text": "Drag the block onto your code space"
            },
            "beacon": {
                "target": {
                    "flyout_block": "every_x_seconds"
                }
            },
            "validation": {
                "blockly": {
                    "create": {
                        "type": "every_x_seconds",
                        "id": "block_1"
                    }
                }
            }
        },
        {
            "banner": {
                "text": "Connect it to this block"
            },
            "beacon": {
                "target": {
                    "block": {
                        "id": null,
                        "rawId": "default_part_event_id"
                    }
                }
            },
            "validation": {
                "blockly": {
                    "connect": {
                        "parent": {
                            "id": null,
                            "rawId": "default_part_event_id",
                            "inputName": "DO"
